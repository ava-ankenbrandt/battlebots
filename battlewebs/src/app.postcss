/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

*{
    @apply: select-none
}

html, body {
  overscroll-behavior-y: none;
}

body {
  touch-action: none;
  /* min-height: -webkit-fill-available; */
  height: 99%;
  width: 99%;
  overflow: hidden;
  -webkit-text-size-adjust: 100%;
}